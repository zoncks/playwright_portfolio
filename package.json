{
  "name": "playwright_portfolio",
  "version": "1.0.0",
  "description": "Comprehensive UI testing suite using Playwright for demonstration purposes",
  "main": "index.js",
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@axe-core/playwright": "^4.7.3",
    "prettier": "^2.8.0"
  },
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=Android --project=iOS",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "format": "prettier --write ."
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "e2e",
    "ui-testing"
  ],
  "author": "",
  "license": "ISC"
}
